{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/dns-lookup/(.*)",
      "dest": "/dns-lookup/$1"
    },
    {
      "src": "/ipsubnet-calculator/(.*)",
      "dest": "/ipsubnet-calculator/$1"
    },
    {
      "src": "/ping-latency-checker/(.*)",
      "dest": "/ping-latency-checker/$1"
    },
    {
      "src": "/whats-my-ip/(.*)",
      "dest": "/whats-my-ip/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
