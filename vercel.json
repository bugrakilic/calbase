{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    },
    {
      "src": "dns-lookup/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "ipsubnet-calculator/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "ping-latency-checker/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "whats-my-ip/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "run.py"
    },
    {
      "src": "/dns-lookup/(.*)",
      "dest": "/mergen/dns-lookup/app.py"
    },
    {
      "src": "/ipsubnet-calculator/(.*)",
      "dest": "/mergen/ipsubnet-calculator/app.py"
    },
    {
      "src": "/ping-latency-checker/(.*)",
      "dest": "/mergen/ping-latency-checker/app.py"
    },
    {
      "src": "/whats-my-ip/(.*)",
      "dest": "/mergen/whats-my-ip/app.py"
    }
  ]
}
